{
  "name": "user",
  "base": "User",
  "relations": {
    "accessTokens": {
      "model": "accessToken",
      "type": "hasMany",
      "foreignKey": "userId"
    },
    "transactions": {
      "model": "transaction",
      "type": "hasMany"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "permission": "DENY",
      "principalType": "ROLE",
      "principalId": "$everyone"
    }
  ],
  "replaceOnPUT": false
}
